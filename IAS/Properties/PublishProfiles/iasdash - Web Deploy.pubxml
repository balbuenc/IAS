<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://iasdash.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>True</ExcludeApp_Data>
    <MSDeployServiceURL>iasdash.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>iasdash</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>False</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$iasdash</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="IASDBContext" Order="1" Enabled="False">
          <Destination Path="Data Source=aibsql.cloudapp.net;Initial Catalog=ias_developer;Persist Security Info=True;User ID=ias_desarrollo;Password=Passw0rd" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="IAS.Models.ApplicationDbContext, IAS" MigrationConfiguration="IAS.Migrations.Configuration, IAS" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)IASDBContext-Web.config Connection String">
      <ParameterValue>Data Source=aibsql.cloudapp.net;Initial Catalog=ias_developer;Persist Security Info=True;User ID=ias_desarrollo;Password=Passw0rd</ParameterValue>
      <UpdateDestWebConfig>False</UpdateDestWebConfig>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>